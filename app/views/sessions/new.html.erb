<div id="backpic">
  <div class="row"><br><br><br><br><br><br><br><br><br><br><br><br><br>
    <div class="col-sm-4 col-sm-offset-4 col-md-4 col-md-offset-4">
      <div id="session">
      <%= form_tag sessions_path do  %>
          <div class="form-group">
            <p><span class="fa fa-user"></span><%= email_field_tag :email, nil, class: "form-control"%></p>
          </div>
          <div class="form-group">
            <p><span class="fa fa-lock"></span><%= password_field_tag :password, nil, class: "form-control"%></p>
          </div><br>
          <div class="row si-btn">
          <div class="col-md-12 col-sm-12">
            <div class="col-sm-5 col-md-2 col-md-offset-2">
            <%= submit_tag "Log In", class: "btn btn-lg btn-primary"%>
          </div>
          <div class="col-sm-5 col-md-2 col-md-offset-2">
            <%= link_to "Sign Up", new_user_path, class: "btn btn-lg btn-success" %>
          </div>
        </div>
      <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
